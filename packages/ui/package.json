{
  "name": "@3squared/forge-ui-3",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/esm/forge-ui-3.mjs",
  "module": "./dist/esm/forge-ui-3.mjs",
  "types": "./dist/types/index.ts",
  "scripts": {
    "dev": "vite",
    "test": "cypress run --component && vitest run --coverage && node ./scripts/merge.js",
    "test:component": "cypress open --component --browser chrome",
    "test:unit": "vitest --ui --coverage",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.11.8",
    "@vueuse/components": "^10.6.1",
    "bootstrap": "5.3.2",
    "primevue": "^3.44.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vue-multiselect": "^3.0.0-beta.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.3",
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "cypress": "^13.4.0",
    "cypress-real-events": "^1.11.0",
    "sass": "^1.69.3",
    "typescript": "^5.0.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.0",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.24"
  },
  "nyc": {
    "all": true,
    "report-dir": "coverage/cypress",
    "include": "*",
    "exclude": "**/*.cy.ts",
    "extension": [
      ".js",
      ".cjs",
      ".mjs",
      ".ts",
      ".tsx",
      ".jsx",
      ".vue"
    ],
    "reporter": [
      "json",
      "cobertura"
    ]
  }
}
