    "vue-router": "4.2.5",
    "rgb-hex": "4.1.0"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "@vitejs/plugin-vue": "4.6.2",
    "typescript": "4.9.5",
    "unplugin-vue-router": "0.7.0",
    "vite": "^4.5.0",
    "vue-tsc": "1.8.27"
  }
