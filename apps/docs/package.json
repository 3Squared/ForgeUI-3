    "lint:fix": "eslint . --ext .ts,.vue --fix"
  },
  "dependencies": {
    "@3squared/forge-playground-3": "*",
    "@3squared/forge-ui-3": "*",
    "@iconify/vue": "4.3.0",
    "rgb-hex": "4.1.0",
    "unplugin-vue-markdown": "0.26.3",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.2.1",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "5.2.3",
    "typescript": "5.7.3",
    "unplugin-vue-router": "0.10.9",
    "vite": "5.4.12",
    "vue-tsc": "2.2.0"
  }
}
