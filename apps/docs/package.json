{
  "name": "forge-ui-3-styleguide",
  "version": "2.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "preview": "npm run build && vite preview",
    "build-bundle": "cd .. && npm i && npm run build && cd docs",
    "build": "vite build --base=/ForgeUI-3/",
